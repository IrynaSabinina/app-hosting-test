{
  "name": "firebase-app-hosting-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nx build website",
    "dev": "nx serve website"
  },
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/website"
      }
    }
  },
  "dependencies": {
    "next": "15.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.0.0",
    "typescript": "^5"
  }
}
